{
  "name": "waku-jotai",
  "description": "Jotai bindings for Waku",
  "version": "0.1.1",
  "type": "module",
  "packageManager": "pnpm@10.12.1",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/wakujs/waku-jotai.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./minimal": {
      "types": "./dist/minimal/index.d.ts",
      "default": "./dist/minimal/index.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "default": "./dist/router/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "rm -rf dist && tsc -p .",
    "test": "pnpm run '/^test:.*/'",
    "test:format": "prettier -c .",
    "test:lint": "eslint .",
    "test:types": "tsc -p . --noEmit",
    "test:types:examples": "tsc -p examples --noEmit",
    "test:spec": "vitest run",
    "examples:01_minimal": "(cd examples/01_minimal; waku dev)",
    "examples:02_router": "(cd examples/02_router; waku dev)"
  },
  "keywords": [
    "react",
    "waku",
    "jotai"
  ],
  "license": "MIT",
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.0.11",
    "jotai": "^2.15.2",
    "prettier": "^3.7.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-server-dom-webpack": "^19.2.1",
    "ts-expect": "^1.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.15",
    "waku": "^0.27.3",
    "waku-jotai": "link:"
  },
  "peerDependencies": {
    "jotai": ">=2.15.2",
    "react": ">=19.0.0",
    "waku": ">=0.23.6"
  }
}
