{
  "name": "waku-jotai",
  "description": "Jotai bindings for Waku",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@10.12.1",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/wakujs/waku-jotai.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./minimal": {
      "types": "./dist/minimal/index.d.ts",
      "default": "./dist/minimal/index.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "default": "./dist/router/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "rm -rf dist && tsc -p tsconfig.json",
    "test": "pnpm run '/^test:.*/'",
    "test:format": "prettier -c .",
    "test:lint": "eslint .",
    "test:types": "tsc -p . --noEmit",
    "test:types:examples": "tsc -p examples --noEmit",
    "test:spec": "vitest run",
    "examples:01_minimal": "(cd examples/01_minimal; waku dev)",
    "examples:02_router": "(cd examples/02_router; waku dev)"
  },
  "keywords": [
    "react",
    "waku",
    "jotai"
  ],
  "license": "MIT",
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.1",
    "happy-dom": "^18.0.1",
    "jotai": "^2.12.5",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-server-dom-webpack": "^19.1.0",
    "ts-expect": "^1.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.5",
    "vitest": "^3.2.4",
    "waku": "^0.23.4",
    "waku-jotai": "link:"
  },
  "peerDependencies": {
    "jotai": ">=2.0.0",
    "waku": ">=0.23.4"
  }
}
